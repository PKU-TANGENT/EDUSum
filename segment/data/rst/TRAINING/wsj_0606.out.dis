( Root (span 1 57)
  ( Nucleus (span 1 48) (rel2par span)
    ( Nucleus (span 1 27) (rel2par span)
      ( Nucleus (span 1 20) (rel2par span)
        ( Nucleus (span 1 12) (rel2par span)
          ( Satellite (span 1 3) (rel2par summary-s)
            ( Satellite (leaf 1) (rel2par hypothetical) (text _!In what could prove a major addition to the Philippines' foreign-investment portfolio,_!) )
            ( Nucleus (span 2 3) (rel2par span)
              ( Nucleus (leaf 2) (rel2par span) (text _!a Taiwanese company signed a $180 million construction contract_!) )
              ( Satellite (leaf 3) (rel2par elaboration-object-attribute-e) (text _!to build the centerpiece of a planned petrochemical complex.<P>_!) )
            )
          )
          ( Nucleus (span 4 12) (rel2par span)
            ( Nucleus (span 4 9) (rel2par span)
              ( Nucleus (span 4 5) (rel2par span)
                ( Nucleus (leaf 4) (rel2par span) (text _!Taiwan's USI Far East Corp., a petrochemical company, initialed the agreement with an unidentified Japanese contractor_!) )
                ( Satellite (leaf 5) (rel2par elaboration-object-attribute-e) (text _!to build a naphtha cracker,_!) )
              )
              ( Satellite (span 6 9) (rel2par attribution)
                ( Nucleus (leaf 6) (rel2par span) (text _!according to Alson Lee,_!) )
                ( Satellite (span 7 9) (rel2par elaboration-object-attribute-e)
                  ( Nucleus (leaf 7) (rel2par span) (text _!who heads the Philippine company_!) )
                  ( Satellite (span 8 9) (rel2par elaboration-object-attribute-e)
                    ( Nucleus (leaf 8) (rel2par span) (text _!set up_!) )
                    ( Satellite (leaf 9) (rel2par purpose) (text _!to build and operate the complex._!) )
                  )
                )
              )
            )
            ( Satellite (span 10 12) (rel2par elaboration-additional)
              ( Nucleus (span 10 11) (rel2par span)
                ( Satellite (leaf 10) (rel2par attribution) (text _!Mr. Lee, president of Luzon Petrochemical Corp., said_!) )
                ( Nucleus (leaf 11) (rel2par span) (text _!the contract was signed Wednesday in Tokyo with USI Far East officials._!) )
              )
              ( Satellite (leaf 12) (rel2par elaboration-additional) (text _!Contract details, however, haven't been made public._!) )
            )
          )
        )
        ( Satellite (span 13 20) (rel2par elaboration-additional)
          ( Nucleus (span 13 16) (rel2par span)
            ( Nucleus (leaf 13) (rel2par span) (text _!The complex is to be located in Batangas, about 70 miles south of Manila.<P>_!) )
            ( Satellite (span 14 16) (rel2par elaboration-additional)
              ( Nucleus (leaf 14) (rel2par span) (text _!USI Far East will hold a 60% stake in Luzon Petrochemical,_!) )
              ( Satellite (span 15 16) (rel2par attribution)
                ( Nucleus (leaf 15) (rel2par span) (text _!according to papers_!) )
                ( Satellite (leaf 16) (rel2par elaboration-object-attribute-e) (text _!signed with the Philippine government's Board of Investments.<P>_!) )
              )
            )
          )
          ( Satellite (span 17 20) (rel2par elaboration-additional)
            ( Nucleus (leaf 17) (rel2par span) (text _!The proposed petrochemical plant would use naphtha_!) )
            ( Satellite (span 18 20) (rel2par purpose)
              ( Nucleus (leaf 18) (rel2par span) (text _!to manufacture the petrochemicals propylene and ethylene and their resin derivatives, polypropylene and polyethylene._!) )
              ( Satellite (span 19 20) (rel2par elaboration-additional)
                ( Nucleus (leaf 19) (rel2par span) (text _!These are the raw materials_!) )
                ( Satellite (leaf 20) (rel2par elaboration-object-attribute-e) (text _!used in making plastics.<P>_!) )
              )
            )
          )
        )
      )
      ( Satellite (span 21 27) (rel2par elaboration-additional)
        ( Nucleus (span 21 26) (rel2par span)
          ( Nucleus (span 21 23) (rel2par span)
            ( Nucleus (leaf 21) (rel2par span) (text _!The contract signing represented a major step in the long-planned petrochemical project._!) )
            ( Satellite (span 22 23) (rel2par elaboration-additional)
              ( Nucleus (leaf 22) (rel2par span) (text _!At an estimated $360 million, the project would represent the single largest foreign investment in the Philippines_!) )
              ( Satellite (leaf 23) (rel2par temporal-after) (text _!since President Corazon Aquino took office in February 1986._!) )
            )
          )
          ( Satellite (span 24 26) (rel2par elaboration-additional)
            ( Nucleus (leaf 24) (rel2par span) (text _!It also is considered critical to the country's efforts_!) )
            ( Satellite (span 25 26) (rel2par elaboration-object-attribute-e)
              ( Nucleus (leaf 25) (rel2par List) (text _!to both attract other investment from Taiwan_!) )
              ( Nucleus (leaf 26) (rel2par List) (text _!and raise heavy industry capabilities._!) )
            )
          )
        )
        ( Satellite (leaf 27) (rel2par background) (text _!The project has been in and out of the pipeline for more than a decade.<P>_!) )
      )
    )
    ( Satellite (span 28 48) (rel2par elaboration-additional)
      ( Satellite (span 28 41) (rel2par antithesis)
        ( Nucleus (leaf 28) (rel2par span) (text _!However, workers can't break ground_!) )
        ( Satellite (span 29 41) (rel2par condition)
          ( Nucleus (span 29 32) (rel2par span)
            ( Nucleus (span 29 30) (rel2par Same-Unit)
              ( Nucleus (leaf 29) (rel2par span) (text _!until legal maneuvers_!) )
              ( Satellite (leaf 30) (rel2par elaboration-object-attribute-e) (text _!to block the complex_!) )
            )
            ( Nucleus (span 31 32) (rel2par Same-Unit)
              ( Nucleus (leaf 31) (rel2par span) (text _!are resolved, moves_!) )
              ( Satellite (leaf 32) (rel2par elaboration-object-attribute-e) (text _!which caused the signing to remain questionable up to the last moment._!) )
            )
          )
          ( Satellite (span 33 41) (rel2par example)
            ( Nucleus (span 33 38) (rel2par List)
              ( Nucleus (span 33 36) (rel2par span)
                ( Nucleus (span 33 35) (rel2par span)
                  ( Satellite (leaf 33) (rel2par circumstance) (text _!As previously reported,_!) )
                  ( Nucleus (span 34 35) (rel2par span)
                    ( Nucleus (leaf 34) (rel2par span) (text _!a member of the Philippines' House of Representatives has sued_!) )
                    ( Satellite (leaf 35) (rel2par purpose) (text _!to stop the plant._!) )
                  )
                )
                ( Satellite (leaf 36) (rel2par background) (text _!The legislator, Enrique Garcia, had actively backed the plant, but at the original site in his constituency northwest of Manila._!) )
              )
              ( Satellite (span 37 38) (rel2par elaboration-additional)
                ( Satellite (leaf 37) (rel2par antithesis) (text _!The country's Supreme Court dismissed the suit,_!) )
                ( Nucleus (leaf 38) (rel2par span) (text _!but Mr. Garcia late last month filed for a reconsideration.<P>_!) )
              )
            )
            ( Nucleus (span 39 41) (rel2par List)
              ( Nucleus (leaf 39) (rel2par span) (text _!In addition, President Aquino has yet to sign into law a bill_!) )
              ( Satellite (span 40 41) (rel2par elaboration-object-attribute-e)
                ( Nucleus (leaf 40) (rel2par span) (text _!removing a stiff 48% tax on naphtha, the principal raw material_!) )
                ( Satellite (leaf 41) (rel2par elaboration-object-attribute-e) (text _!to be used in the cracker.<P>_!) )
              )
            )
          )
        )
      )
      ( Nucleus (span 42 48) (rel2par span)
        ( Nucleus (span 42 44) (rel2par span)
          ( Nucleus (leaf 42) (rel2par List) (text _!However, at a news conference Thursday, Mrs. Aquino backed the project_!) )
          ( Nucleus (span 43 44) (rel2par List)
            ( Satellite (leaf 43) (rel2par attribution) (text _!and said_!) )
            ( Nucleus (leaf 44) (rel2par span) (text _!her government was attempting to soothe the feelings of residents at the original site, adjacent to the government's major petroleum refinery in Bataan province._!) )
          )
        )
        ( Satellite (span 45 48) (rel2par elaboration-additional)
          ( Nucleus (span 45 47) (rel2par span)
            ( Satellite (leaf 45) (rel2par attribution) (text _!"We have tried our best to tell the people in Bataan_!) )
            ( Nucleus (span 46 47) (rel2par span)
              ( Nucleus (leaf 46) (rel2par Problem-Solution) (text _!that maybe this time it will not go to them,_!) )
              ( Nucleus (leaf 47) (rel2par Problem-Solution) (text _!but certainly we will do our best to encourage other investors to go to their province,"_!) )
            )
          )
          ( Satellite (leaf 48) (rel2par attribution) (text _!Mrs. Aquino told Manila-based foreign correspondents.<P>_!) )
        )
      )
    )
  )
  ( Satellite (span 49 57) (rel2par elaboration-additional)
    ( Nucleus (span 49 54) (rel2par span)
      ( Satellite (span 49 51) (rel2par antithesis)
        ( Nucleus (leaf 49) (rel2par span) (text _!The project appeared to be on the rocks earlier this month_!) )
        ( Satellite (span 50 51) (rel2par circumstance)
          ( Nucleus (leaf 50) (rel2par span) (text _!when the other major partner in the project, China General Plastics Corp., backed out._!) )
          ( Satellite (leaf 51) (rel2par elaboration-additional) (text _!China General Plastics, another Taiwanese petrochemical manufacturer, was to have a 40% stake in Luzon Petrochemical.<P>_!) )
        )
      )
      ( Nucleus (span 52 54) (rel2par span)
        ( Satellite (leaf 52) (rel2par attribution) (text _!However, Mr. Lee said_!) )
        ( Nucleus (span 53 54) (rel2par span)
          ( Satellite (leaf 53) (rel2par attribution) (text _!that USI Far East is confident_!) )
          ( Nucleus (leaf 54) (rel2par span) (text _!other investors will take up the slack._!) )
        )
      )
    )
    ( Satellite (span 55 57) (rel2par elaboration-additional)
      ( Satellite (leaf 55) (rel2par attribution) (text _!He said_!) )
      ( Nucleus (span 56 57) (rel2par span)
        ( Nucleus (leaf 56) (rel2par span) (text _!USI Far East has applied to both the Asian Development Bank and the World Bank's International Finance Corp. for financing_!) )
        ( Satellite (leaf 57) (rel2par elaboration-object-attribute-e) (text _!that could include equity stakes._!) )
      )
    )
  )
)
